<%= link_to "create", new_task_path %>
<br>
<% @tasks.each do |task| %>
  <p>Task Id:  <%= task.id %><p>
  <p>Note:  <%= task.note %><p>
  <p>Due:  <%= task.due_date %><p>
  <%= link_to "view", task_path(task.id) %>
  <%= link_to "edit", edit_task_path(task.id) %>
  <%= link_to "destroy", task_path(task.id), method: :delete %>
  <br>
  <br>
  <br>
<% end %>
